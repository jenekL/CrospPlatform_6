<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.TclientsEntity" table="tclients" schema="krosp_lab6">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="firm">
            <column name="Firm" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="country">
            <column name="Country" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="adress">
            <column name="Adress" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="telephone">
            <column name="Telephone" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
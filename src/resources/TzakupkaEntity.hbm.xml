<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.TzakupkaEntity" table="tzakupka" schema="krosp_lab6">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="namee">
            <column name="Namee" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="float" precision="-1" not-null="true"/>
        </property>
        <property name="ed">
            <column name="Ed" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>